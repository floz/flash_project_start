package com.makemepulse.ui.form.validator{    /**     * @author Nicolas Rajabaly - nicolas@makemepulse.com     */    public class LenghValidator implements IValidator {    	    	private var _min:Number;    	private var _max:Number;    	    	public function LenghValidator(min:Number,max:Number){    		_min=min;    		_max=max;    	}	    	    	        public function isValid (o : * = null) : Boolean {        	if(o==null) return false;            var str:String=o.toString();            if(str.length<_min) return false;            if(str.length>_max) return false;            return true;    	}    }}