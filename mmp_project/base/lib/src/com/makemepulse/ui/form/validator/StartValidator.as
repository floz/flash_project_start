package com.makemepulse.ui.form.validator{        /**     * @author Nicolas Rajabaly - nicolas@makemepulse.com     */    public class StartValidator implements IValidator {        private var _start : String;                public function StartValidator(start:String){    		_start=start;    	}	    	    	        public function isValid (o : * = null) : Boolean {            if(o==null) return false;            var str:String=o.toString();            if(str.substr(0,_start.length)==_start) return true;            return false;        }    }}