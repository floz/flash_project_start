package com.makemepulse.ui.form {    /**     * @author Nicolas Rajabaly - nicolas@makemepulse.com     */    public class FormDefaultValue {    	    	private var _value:* =null;    	private var _valid:Boolean;    	    	public function FormDefaultValue(value:*=null,valid:Boolean=true){    		_value=value;    		_valid=valid;    	}    	    	public function get value():*{    		return _value;    	}    	    	public function isValid():Boolean{    		return _valid;    	}    	    }}